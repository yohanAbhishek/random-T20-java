<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PostGameScenes.PostGameController">
    <TableView fx:id="summaryTable" layoutX="130.0" layoutY="150.0" prefHeight="91.0" prefWidth="348.0">
      <columns>
          <TableColumn fx:id="scoreColumn" prefWidth="76.0" text="Score" />
          <TableColumn fx:id="ballsColumn" prefWidth="86.0" text="Total balls" />
          <TableColumn fx:id="wicketsColumn" prefWidth="95.0" text="Total Wickets" />
          <TableColumn fx:id="winColumn" prefWidth="90.0" text="Win" />
      </columns>
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
    </TableView>
    <Button alignment="CENTER" contentDisplay="CENTER" layoutX="130.0" layoutY="251.0" mnemonicParsing="false" onAction="#loadSummary" prefHeight="27.0" prefWidth="348.0" style="-fx-background-color: black;" text="Load" textFill="WHITE">
      <font>
         <Font name="Arial Italic" size="13.0" />
      </font></Button>
   <Label fx:id="team1Label" alignment="CENTER" contentDisplay="CENTER" layoutX="58.0" layoutY="181.0" prefHeight="18.0" prefWidth="73.0" textFill="WHITE">
      <font>
         <Font name="Arial Bold Italic" size="13.0" />
      </font></Label>
   <Label fx:id="team2Label" alignment="CENTER" contentDisplay="CENTER" layoutX="57.0" layoutY="206.0" prefHeight="18.0" prefWidth="73.0" textFill="WHITE">
      <font>
         <Font name="Arial Bold Italic" size="13.0" />
      </font></Label>
</AnchorPane>
