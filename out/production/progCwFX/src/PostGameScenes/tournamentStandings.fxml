<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PostGameScenes.PostGameController">
    <TableView fx:id="tournamentTable" layoutX="130.0" layoutY="42.0" prefHeight="258.0" prefWidth="348.0">
        <columns>
            <TableColumn fx:id="columnTournamentTeam" prefWidth="174.0" text="Team" />
            <TableColumn fx:id="columnTournamentScore" prefWidth="173.0" text="Total score" />
        </columns>
    </TableView>
    <Button alignment="CENTER" contentDisplay="CENTER" layoutX="130.0" layoutY="314.0" mnemonicParsing="false" onAction="#loadTournamentStanding" prefHeight="26.0" prefWidth="237.0" style="-fx-background-color: black;" text="Load" textFill="WHITE">
      <font>
         <Font name="Arial Italic" size="13.0" />
      </font></Button>
    <Button layoutX="376.0" layoutY="314.0" mnemonicParsing="false" onAction="#handleSummaryChartButton" prefHeight="26.0" prefWidth="103.0" style="-fx-background-color: black;" text="View Chart" textFill="WHITE">
        <font>
            <Font name="Arial Italic" size="13.0" />
        </font>
    </Button>
</AnchorPane>
