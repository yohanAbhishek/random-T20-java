<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PostGameScenes.PostGameController">
    <Button layoutX="524.0" layoutY="358.0" mnemonicParsing="false" onAction="#loadDataIntoChart" prefHeight="34.0" prefWidth="62.0" text="LOAD" />
    <BarChart fx:id="scoreSummaryChart" layoutX="87.0" layoutY="25.0" prefHeight="350.0" prefWidth="427.0">
      <xAxis>
          <CategoryAxis fx:id="groupAxis" label="TEAM" tickLabelGap="20.0" />
      </xAxis>
      <yAxis>
          <NumberAxis fx:id="scoreAxis" label="SCORE" side="LEFT" />
      </yAxis>
    </BarChart>
</AnchorPane>
